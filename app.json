{
  "expo": {
    "name": "Friendzy",
    "slug": "Friendzy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/tyuss/loginLogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#000000CC"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pjamalapurapu.Friendzy"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/tyuss/loginLogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.pjamalapurapu.Friendzy"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/tyuss/loginLogo.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "cde19659-6f3f-44ca-8bcb-034b68fe3234"
      }
    }
  }
}
